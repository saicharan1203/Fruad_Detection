/* Alert System Styles */
.alert-system {
  position: fixed;
  top: 20px;
  right: 20px;
  z-index: 1002;
}

.alert-button {
  width: 50px;
  height: 50px;
  border-radius: 50%;
  background: linear-gradient(135deg, var(--primary), var(--secondary));
  color: white;
  border: none;
  cursor: pointer;
  display: flex;
  align-items: center;
  justify-content: center;
  box-shadow: 0 4px 15px rgba(106, 17, 203, 0.3);
  position: relative;
  transition: all 0.3s ease;
}

.alert-button:hover {
  transform: scale(1.1);
  box-shadow: 0 6px 20px rgba(106, 17, 203, 0.5);
}

.alert-button.has-unread::after {
  content: '';
  position: absolute;
  top: -2px;
  right: -2px;
  width: 12px;
  height: 12px;
  background: #ff4757;
  border-radius: 50%;
  border: 2px solid white;
  animation: pulse 2s infinite;
}

