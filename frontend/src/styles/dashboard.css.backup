/* Live Dashboard Styles */
.live-dashboard-container {
  margin: 20px 0;
  padding: 25px;
  background: white;
  border-radius: 15px;
  box-shadow: 0 4px 20px rgba(0, 0, 0, 0.1);
  width: 100%;
  max-width: 100%;
  overflow-x: hidden;
  box-sizing: border-box;
}

.stats-grid-live {
  display: grid;
  grid-template-columns: repeat(auto-fit, minmax(250px, 1fr));
  gap: 20px;
  margin-bottom: 30px;
}

.stat-card-live {
  display: flex;
  gap: 20px;
  padding: 25px;
  border-radius: 12px;
  background: white;
  border: 2px solid #e0e0e0;
  transition: all 0.3s ease;
  position: relative;
  overflow: hidden;
}

.stat-card-live::before {
  content: '';
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  height: 4px;
  background: linear-gradient(90deg, var(--primary), var(--secondary));
}

.stat-card-live.primary::before {
  background: linear-gradient(90deg, #6a11cb, #2575fc);
}

.stat-card-live.danger::before {
  background: linear-gradient(90deg, #ff4757, #ff6b81);
}

.stat-card-live.success::before {
  background: linear-gradient(90deg, #2ed573, #7bed9f);
}

.stat-card-live.warning::before {
  background: linear-gradient(90deg, #ffa502, #ffd93d);
}

.stat-card-live:hover {
  transform: translateY(-5px);
  box-shadow: 0 8px 30px rgba(0, 0, 0, 0.15);
}

.stat-icon-wrapper {
  width: 60px;
  height: 60px;
  border-radius: 12px;
  display: flex;
  align-items: center;
  justify-content: center;
  background: linear-gradient(135deg, rgba(106, 17, 203, 0.1), rgba(37, 117, 252, 0.15));
  color: var(--primary);
