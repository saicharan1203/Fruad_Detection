/* Upload Section */
.upload-section {
  margin-bottom: 40px;
  animation: fadeIn 0.6s ease-out;
}

.upload-card {
  background: white;
  border-radius: 15px;
  padding: 40px;
  box-shadow: 0 10px 40px rgba(0, 0, 0, 0.1);
  text-align: center;
  transition: transform 0.3s ease, box-shadow 0.3s ease;
}

.upload-card:hover {
  transform: translateY(-5px);
  box-shadow: 0 15px 50px rgba(0, 0, 0, 0.15);
  animation: glow 1s ease-in-out;
}

.upload-icon {
  font-size: 3em;
  color: var(--primary);
  margin-bottom: 20px;
  animation: bounce 2s infinite;
}

@keyframes bounce {
  0%, 100% {
    transform: translateY(0);
  }
  50% {
    transform: translateY(-10px);
  }
}

.upload-card h2 {
  margin-bottom: 20px;
  color: var(--dark);
}

.file-input-wrapper {
  position: relative;
  margin: 30px 0;
  display: flex;
  justify-content: center;
}

.file-label {
  display: inline-block;
  padding: 20px 40px;
  border: 3px dashed var(--primary);
  border-radius: 15px;
  cursor: pointer;
  background: rgba(106, 17, 203, 0.1);
  transition: all 0.3s ease;
  font-weight: 600;
  font-size: 1.1em;
  color: var(--primary);
  text-align: center;
  box-shadow: 0 5px 15px rgba(106, 17, 203, 0.2);
  position: relative;
  overflow: hidden;
}

.file-label::before {
  content: '';
  position: absolute;
  top: 0;
  left: -100%;
  width: 100%;
  height: 100%;
  background: linear-gradient(90deg, transparent, rgba(255, 255, 255, 0.3), transparent);
  transition: 0.5s;
}

.file-label:hover {
  background: rgba(106, 17, 203, 0.2);
  border-color: var(--secondary);
  transform: scale(1.05);
  box-shadow: 0 8px 25px rgba(106, 17, 203, 0.4);
  animation: pulse 1s ease-in-out;
}

.file-label:hover::before {
  left: 100%;
}

.file-label-highlight {
  animation: bounce 2s infinite;
  position: relative;
}

.file-label-highlight::after {
  content: '';
  position: absolute;
  top: -5px;
  left: -5px;
  right: -5px;
  bottom: -5px;
  border: 2px solid var(--secondary);
  border-radius: 20px;
  animation: glow 2s infinite alternate;
  z-index: -1;
}

.button-group {
  display: flex;
  gap: 15px;
  justify-content: center;
  margin: 30px 0;
  flex-wrap: wrap;
}

/* Enhanced Button Animations */
.btn {
  padding: 12px 30px;
  border: none;
  border-radius: 8px;
  cursor: pointer;
  font-weight: 600;
  transition: all 0.3s ease;
  font-size: 1em;
  display: inline-flex;
  align-items: center;
  gap: 8px;
  position: relative;
  overflow: hidden;
  transform: translateZ(0);
  animation: pulse 2s infinite;
  box-shadow: 0 4px 6px rgba(0, 0, 0, 0.1);
}

.btn::before {
  content: '';
  position: absolute;
  top: 0;
  left: -100%;
  width: 100%;
  height: 100%;
  background: linear-gradient(90deg, transparent, rgba(255, 255, 255, 0.2), transparent);
  transition: 0.5s;
}

.btn:hover::before {
  left: 100%;
}

.btn-primary {
  background: var(--primary);
  color: white;
  background: linear-gradient(135deg, var(--primary), #2b8bb1);
}

.btn-primary:hover:not(:disabled) {
  background: linear-gradient(135deg, #2b8bb1, var(--primary));
  transform: translateY(-2px);
  box-shadow: 0 8px 15px rgba(106, 17, 203, 0.4);
  animation: bounce 0.6s ease;
}

.btn-secondary {
  background: var(--secondary);
  color: white;
  background: linear-gradient(135deg, var(--secondary), #20b95e);
}

.btn-secondary:hover:not(:disabled) {
  background: linear-gradient(135deg, #1a5fc4, var(--secondary));
  transform: translateY(-2px);
  box-shadow: 0 8px 15px rgba(37, 117, 252, 0.4);
  animation: bounce 0.6s ease;
}

.btn-sm {
  padding: 8px 16px;
  font-size: 0.9em;
}

.btn-large {
  padding: 15px 40px;
  font-size: 1.1em;
}

.btn:disabled {
  opacity: 0.5;
  cursor: not-allowed;
  animation: none;
}

.btn-loading {
  animation: spin 1s linear infinite;
  display: inline-block;
}

.message {
  padding: 15px 20px;
  border-radius: 8px;
  margin: 20px 0;
  display: flex;
  align-items: center;
  gap: 10px;
  font-weight: 500;
  animation: slideDown 0.4s ease-out;
}

.message-success {
  background: rgba(0, 176, 155, 0.15);
  color: var(--success);
  border-left: 4px solid var(--success);
}

.message-error {
  background: rgba(255, 65, 108, 0.15);
  color: var(--danger);
  border-left: 4px solid var(--danger);
}

.error-message {
  background: rgba(255, 65, 108, 0.15);
  color: var(--danger);
  border-left: 4px solid var(--danger);
  padding: 15px 20px;
  border-radius: 8px;
  margin: 20px 0;
  display: flex;
  align-items: center;
  gap: 10px;
  font-weight: 500;
  animation: slideDown 0.4s ease-out;
}

.file-info {
