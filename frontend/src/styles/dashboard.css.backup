  gap: 15px;
  flex-wrap: wrap;
  margin-top: 25px;
}

.btn-danger {
  padding: 12px 25px;
  background: linear-gradient(135deg, #ff4757, #ff6b81);
  color: white;
  border: none;
  border-radius: 8px;
  font-weight: 600;
  cursor: pointer;
  transition: all 0.3s ease;
}

.btn-danger:hover {
  transform: translateY(-2px);
  box-shadow: 0 6px 20px rgba(255, 71, 87, 0.3);
}

.btn-secondary {
  padding: 12px 25px;
  background: white;
  color: var(--dark);
  border: 2px solid var(--primary);
  border-radius: 8px;
  font-weight: 600;
  cursor: pointer;
  transition: all 0.3s ease;
}

.btn-secondary:hover {
  background: var(--primary);
  color: white;
  transform: translateY(-2px);
}

.api-group {
  margin-bottom: 35px;
}

.api-group h3 {
  color: var(--dark);
  margin-bottom: 20px;
  font-size: 1.2em;
}

.input-with-action {
  display: flex;
  gap: 10px;
}

.input-with-action input {
  flex: 1;
}

.btn-generate {
  padding: 12px 20px;
  background: linear-gradient(135deg, var(--primary), var(--secondary));
  color: white;
  border: none;
  border-radius: 8px;
  font-weight: 600;
  cursor: pointer;
  transition: all 0.3s ease;
  white-space: nowrap;
}

.btn-generate:hover {
  transform: translateY(-2px);
  box-shadow: 0 6px 20px rgba(106, 17, 203, 0.3);
}

.api-documentation {
