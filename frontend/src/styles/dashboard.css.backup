  flex-direction: column;
  gap: 20px;
  margin-bottom: 30px;
}

.metric-card {
  padding: 20px;
  background: linear-gradient(135deg, rgba(106, 17, 203, 0.05), rgba(37, 117, 252, 0.05));
  border-radius: 10px;
  border-left: 4px solid var(--primary);
}

.metric-card h3 {
  color: var(--dark);
  margin-bottom: 15px;
  font-size: 1.1em;
}

.metric-bar-container {
  width: 100%;
  height: 40px;
  background: #e0e0e0;
  border-radius: 20px;
  overflow: hidden;
  position: relative;
}

.metric-bar {
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: flex-end;
  padding-right: 15px;
  color: white;
  font-weight: 700;
