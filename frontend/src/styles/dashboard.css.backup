  padding: 25px;
  background: white;
  border-radius: 12px;
  text-align: center;
  box-shadow: 0 4px 15px rgba(0, 0, 0, 0.08);
  transition: all 0.3s ease;
}

.stat-card-activity:hover {
  transform: translateY(-5px);
  box-shadow: 0 8px 25px rgba(0, 0, 0, 0.12);
}

.stat-value {
  font-size: 2.5em;
  font-weight: 700;
  color: var(--primary);
  margin-bottom: 8px;
}

.stat-label {
  color: var(--gray);
  font-size: 0.95em;
  font-weight: 600;
}

.activity-timeline {
  background: white;
  border-radius: 15px;
  padding: 25px;
  box-shadow: 0 4px 20px rgba(0, 0, 0, 0.08);
}

.activity-item {
  display: flex;
  gap: 20px;
  padding: 20px;
  border-radius: 10px;
  margin-bottom: 15px;
  transition: all 0.3s ease;
  border-left: 4px solid;
}

.activity-item.severity-critical {
  background: linear-gradient(to right, rgba(255, 71, 87, 0.05), white);
  border-left-color: #ff4757;
}

.activity-item.severity-warning {
  background: linear-gradient(to right, rgba(255, 165, 2, 0.05), white);
  border-left-color: #ffa502;
}

.activity-item.severity-success {
  background: linear-gradient(to right, rgba(46, 213, 115, 0.05), white);
  border-left-color: #2ed573;
}

.activity-item.severity-info {
  background: linear-gradient(to right, rgba(37, 117, 252, 0.05), white);
  border-left-color: #2575fc;
}

.activity-item:hover {
  transform: translateX(10px);
  box-shadow: 0 4px 15px rgba(0, 0, 0, 0.1);
}

.activity-icon {
  font-size: 2em;
  flex-shrink: 0;
}

.activity-content {
  flex: 1;
}

.activity-header {
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-bottom: 10px;
}

.activity-header h3 {
  color: var(--dark);
  font-size: 1.1em;
  margin: 0;
}

.severity-badge {
  padding: 5px 12px;
  border-radius: 15px;
  font-size: 0.75em;
  font-weight: 700;
  text-transform: uppercase;
}

.severity-badge.critical {
  background: #ff4757;
  color: white;
}

.severity-badge.warning {
  background: #ffa502;
  color: white;
}

.severity-badge.success {
  background: #2ed573;
  color: white;
}

.severity-badge.info {
  background: #2575fc;
  color: white;
}

.activity-details {
  color: var(--gray);
  margin-bottom: 12px;
  line-height: 1.5;
}

.activity-meta {
  display: flex;
  gap: 20px;
  flex-wrap: wrap;
}

.meta-item {
  display: flex;
  align-items: center;
  gap: 5px;
  font-size: 0.85em;
  color: var(--gray);
  font-weight: 600;
}

.type-badge {
  padding: 4px 10px;
  background: linear-gradient(135deg, rgba(106, 17, 203, 0.1), rgba(37, 117, 252, 0.1));
  border-radius: 12px;
  color: var(--primary);
  text-transform: capitalize;
}

.empty-state-activity {
  text-align: center;
  padding: 60px 20px;
}

.empty-state-activity .empty-icon {
  font-size: 4em;
  margin-bottom: 15px;
  opacity: 0.5;
}

.empty-state-activity h3 {
  color: var(--dark);
  margin-bottom: 10px;
}

.empty-state-activity p {
  color: var(--gray);
}

/* Responsive Settings & Activity */
@media (max-width: 1024px) {
  .settings-layout {
    grid-template-columns: 1fr;
  }
  
  .settings-tabs {
    flex-direction: row;
    overflow-x: auto;
  }
  
  .tab-btn:hover {
    transform: none;
  }
}

@media (max-width: 768px) {
  .form-grid {
    grid-template-columns: 1fr;
  }
  
  .profile-photo-section {
    flex-direction: column;
    text-align: center;
  }
  
  .activity-controls {
    flex-direction: column;
  }
  
  .search-box,
  .filter-group {
    width: 100%;
  }
  
  .security-actions {
    flex-direction: column;
  }
  
  .security-actions button {
    width: 100%;
  }
  
  .settings-actions {
    flex-direction: column;
  }
  
  .settings-actions button {
    width: 100%;
  }
}

/* Highlighted Keywords */
.highlight-title {
  background: linear-gradient(135deg, #6a11cb 0%, #2575fc 100%);
  color: white;
  padding: 4px 12px;
  border-radius: 20px;
  font-weight: 700;
  box-shadow: 0 4px 8px rgba(106, 17, 203, 0.3);
  display: inline-block;
  margin: 0 4px;
  animation: pulse 2s infinite;
}

.highlight-action {
  background: linear-gradient(135deg, #2ed573 0%, #1dd1a1 100%);
  color: white;
  padding: 3px 8px;
  border-radius: 6px;
  font-weight: 600;
  box-shadow: 0 2px 6px rgba(46, 213, 115, 0.3);
  display: inline-block;
  margin: 0 3px;
}

.highlight-label {
  background: linear-gradient(135deg, #ff9a00 0%, #ff6b00 100%);
  color: white;
  padding: 2px 6px;
  border-radius: 4px;
  font-weight: 600;
  box-shadow: 0 2px 4px rgba(255, 154, 0, 0.3);
  display: inline-block;
  margin: 0 2px;
}

@keyframes pulse {
  0% { transform: scale(1); }
  50% { transform: scale(1.05); }
  100% { transform: scale(1); }
}
