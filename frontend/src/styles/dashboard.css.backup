  align-items: center;
  gap: 10px;
}

.profile-photo-section {
  display: flex;
  gap: 30px;
  align-items: center;
  padding: 25px;
  background: linear-gradient(135deg, rgba(106, 17, 203, 0.05), rgba(37, 117, 252, 0.05));
  border-radius: 12px;
