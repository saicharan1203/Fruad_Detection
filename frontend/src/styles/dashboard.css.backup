/* Enhanced Animations */
@keyframes pulse {
  0% {
    box-shadow: 0 0 0 0 rgba(61, 165, 196, 0.4);
  }
  70% {
    box-shadow: 0 0 0 10px rgba(61, 165, 196, 0);
  }
  100% {
    box-shadow: 0 0 0 0 rgba(61, 165, 196, 0);
  }
}

@keyframes bounce {
  0%, 20%, 50%, 80%, 100% {
    transform: translateY(0);
  }
  40% {
    transform: translateY(-5px);
  }
  60% {
    transform: translateY(-3px);
  }
}

@keyframes spin {
  0% {
    transform: rotate(0deg);
  }
  100% {
    transform: rotate(360deg);
  }
}

@keyframes glow {
  0% {
    box-shadow: 0 0 5px rgba(37, 117, 252, 0.5);
  }
  50% {
    box-shadow: 0 0 20px rgba(37, 117, 252, 0.8);
  }
  100% {
    box-shadow: 0 0 5px rgba(37, 117, 252, 0.5);
  }
}

@keyframes fadeInOut {
  0%, 100% {
    opacity: 0.5;
  }
  50% {
    opacity: 1;
  }
}

@keyframes slideIn {
  from {
    opacity: 0;
    transform: translateY(20px);
  }
  to {
    opacity: 1;
    transform: translateY(0);
  }
}

