  box-shadow: 0 4px 15px rgba(106, 17, 203, 0.3);
}

.settings-content {
  background: white;
  border-radius: 15px;
  padding: 30px;
  box-shadow: 0 4px 20px rgba(0, 0, 0, 0.08);
}

.settings-section h2 {
