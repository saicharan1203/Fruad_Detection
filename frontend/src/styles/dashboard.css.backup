  grid-template-columns: 250px 1fr;
  gap: 25px;
}

.settings-tabs {
  display: flex;
  flex-direction: column;
  gap: 10px;
}

.tab-btn {
  display: flex;
  align-items: center;
  gap: 12px;
  padding: 15px 20px;
  background: white;
  border: 2px solid #e0e0e0;
  border-radius: 10px;
  cursor: pointer;
  transition: all 0.3s ease;
  font-size: 1em;
  font-weight: 600;
